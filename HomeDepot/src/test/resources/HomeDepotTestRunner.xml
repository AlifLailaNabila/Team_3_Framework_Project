<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="HomeDepot Automation Test" parallel="false">


    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="windows"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="62.0"/>
    <parameter name="url" value="https://www.homedepot.com/"/>

     <test name = "Home Depot">
        <classes>
             <class name="testCases.TestHomePage"/>
           <class name ="testCases.TestLoginPage"/>
            <class name="testCases.TestSearchPage"/>
            <class name = "testCases.TestFooterPage"/>
         <!--<class name = "testCases.TestAddToCartPage"/>-->
        </classes>
    </test>

</suite>